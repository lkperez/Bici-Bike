<div class="page-section flexbox vertical stretch" style="z-index:0;">
  <div class="section-inner flexbox vertical stretch">
    <div class="section-title" style="margin-top:50px">
      Review '<%=Route.find(params[:route_id]).name%>'
    </div>
    <div class="reviews new">
      <%= form_with model: @review do |f| %>
        <%= f.label "Star Rating"%>: <br>
        <%= f.number_field :starRating, min:1, max:5 %> <br><br>
        <%= f.label "Review"%>: <br>
        <%= f.text_area :textReview, size:"70x5" %> <br><br>
        <%= f.hidden_field :route_id, value: params[:route_id]%>
        <%= f.hidden_field :member_id, value: current_member.id%>
        <div class="button-style"><%= f.submit %></div>
      <% end %>
    </div>
	</div>
